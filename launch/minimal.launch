<launch>
  <!-- Load the URDF model -->
  <param name="robot_description" command="cat $(find cw_2)/urdf/minimal.urdf" />

  <!-- Start Gazebo -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch" />

  <!-- Spawn the robot model in Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-param robot_description -urdf -model minimal_robot" />
</launch>